# program to read grid and find S and T

grid = [
    ["-","S","-"],
    ["T","-","T"],
    ["-","-","T"]
]

start = ()
tasks = []

for i in range(len(grid)):
    for j in range(len(grid[i])):
        if grid[i][j]=="S":
            start = (i,j)
        if grid[i][j]=="T":
            tasks.append((i,j))

print("Start is at:", start)
print("Tasks are:", tasks)
